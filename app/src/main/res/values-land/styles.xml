<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Customize your theme here. -->
    <style name="ButtonStyleLand">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:backgroundTint">@color/textViewBackGray</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/button_text_size</item>
    </style>

    <style name="ButtonStyle2Land">
        <item name="android:textSize">@dimen/button_text_size</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:backgroundTint">@color/textViewBackOrange</item>
    </style>


    <style name="EditTextStyleLand" >
        <item name="android:gravity">end</item>
        <item name="android:textSize">@dimen/result_size</item>
        <item name="android:textColor">@color/black</item>

    </style>


</resources>